{
  "name": "Algotrade.io",
  "private": true,
  "version": "1.0.0",
  "description": "Vite app for Algotrade.io",
  "type": "module",
  "scripts": {
    "dev": "pnpm run start",
    "typescript": "tsc --noEmit",
    "build": "pnpm run swagger && pnpm run typescript && NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "preview": "vite preview",
    "start": "vite",
    "ci": "rm -rf node_modules && pnpm install --frozen-lockfile",
    "amp": "APP_ID=\"${APP_ID}\" ENV=\"${ENV}\" amplify/pull.sh",
    "clean": "rm -rf node_modules && rm -rf src/.umi",
    "swagger": "swagger-cli bundle -o src/ui/gen/swagger.json src/api/spec/swagger.yaml",
    "visualize": "vite-bundle-visualizer -t treemap --open -o ${PWD}/stats.html",
    "test": "dotenv -e config.env -- npx cypress open"
  },
  "author": "Krish Suchak <suchak.krish@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^4.8.0",
    "@aws-amplify/ui-react": "^4.3.1",
    "@nfte/react": "^1.0.4",
    "antd": "^5.1.0",
    "aws-amplify": "^5.0.7",
    "plotly.js-dist-min": "^2.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hover-video-player": "^9.7.1",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.6.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-use-websocket": "^4.5.0",
    "styled-components": "^5.3.6",
    "swagger-ui-react": "^4.12.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.7",
    "@types/node": "^20.2.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/styled-components": "^5.1.36",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^13.0.0",
    "cypress-vite": "^1.4.0",
    "less": "^4.1.3",
    "typescript": "^4.9.3",
    "vite": "^4.3.9",
    "vite-plugin-istanbul": "^4.1.0"
  }
}